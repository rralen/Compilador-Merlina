transistor(1)[   //objeto y el numero entre parentecis es el numero del objeto y lo que este adentro de {} son para definirlo
        90º,     //la horientazion del objeto  en grados es decir la inclinacion de este y la coma es para separarlo del siguinte dato
        orientacion(x=5,y=-1) //posicion basada en las cordenadas desde donde se inicia la esquina superior izquierda donde cera x=0,y=0 y asi podremos definir la ubicacion
        1:gnd,          //el objeto "transistor" tiene tres salidas 1,2 y 3 a cada pata se le da un nombre
        2:tetl(1,orientacion(x=15,y=-20)),    //esto llama a otro objeto llamado "tetl" que es un circulo
        3:sor       //a la compuerta 3 se le otorga el nombre de sor
        ]       //al cerrar significa que el objeto se termina de definir
transistor(2)[
        90º,
        orientacion(x=20,y=-1),
        1:sor,          
        2:salida(2),
        3:vcc
        ]

rectangulo(1)[
        90º,
        orientacion(x=20,y=-1)
        ]

rectangulo(3)[
    0º,    // orientación
    posicion(x=1,y=-2) // donde inicia
]